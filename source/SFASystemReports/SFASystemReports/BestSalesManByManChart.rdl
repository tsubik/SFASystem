  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="SalesRep">
          <DataField>SalesRep</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT C.FirstName + ' ' +C.LastName as SalesRep, SUM(ord.Total)/1000 as Sales
FROM [Order] ord INNER JOIN [Employee] emp ON ord.EmployeeID=emp.EmployeeID
INNER JOIN [Employee] emp2 ON emp.ManagerID=emp2.EmployeeID
INNER JOIN [Contact] C ON emp.ContactID=C.ContactID
WHERE emp2.EmployeeID = @ManID
GROUP BY emp.EmployeeID, C.FirstName + ' ' +C.LastName
ORDER BY Sales DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ManID">
            <Value>=Parameters!ManID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
