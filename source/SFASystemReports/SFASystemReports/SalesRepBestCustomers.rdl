  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT cust.Name, SUM(ord.Total) as Sales
FROM [Order] ord INNER JOIN CustomerFacility cFac
ON ord.CustomerFacilityID=cFac.CustomerFacilityID
INNER JOIN [Customer] cust ON cFac.CustomerID=cust.CustomerID
INNER JOIN [Employee] emp ON ord.EmployeeID=emp.EmployeeID
WHERE emp.EmployeeID=@EmpID
GROUP BY cust.CustomerID, cust.Name
ORDER BY Sales DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@EmpID">
            <Value>=Parameters!EmpID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
