      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>WITH RecursionCTE(TerritoryID) 
AS (
	SELECT TerritoryID
	FROM Territory
	WHERE ParentTerritoryID = @territoryid
	UNION ALL
	SELECT t1.TerritoryID
	FROM Territory as t1 
	JOIN RecursionCTE as Rec  on Rec.TerritoryID=t1.ParentTerritoryID 
)


SELECT TOP 10 prod.Name, SUM(ord.Total) as SumTotal
FROM [Product] prod JOIN [OrderLine] ordLine
ON prod.ProductID=ordLine.ProductID 
JOIN [Order] ord ON ordLine.OrderID=ord.OrderID
JOIN [Territory] terr ON ord.TerritoryID=terr.TerritoryID
WHERE ord.OrderDate between @startdate and @enddate AND ord.TerritoryID IN
(

SELECT * FROM RecursionCTE 
UNION ALL
SELECT TerritoryID
FROM Territory
WHERE TerritoryID = @territoryid
)
GROUP BY prod.ProductID, prod.Name
ORDER BY SumTotal DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@territoryid">
            <Value>=Parameters!territoryID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@startdate">
            <Value>=Parameters!startdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@enddate">
            <Value>=Parameters!enddate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
