    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT TOP 10 cust.Name, SUM(ord.Total)/1000 as Sales
FROM [Order] ord INNER JOIN [CustomerFacility] cFac
ON ord.CustomerFacilityID=cFac.CustomerFacilityID
INNER JOIN [Customer] cust ON cFac.CustomerID=cust.CustomerID
Group BY cust.Name
ORDER BY Sales DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
