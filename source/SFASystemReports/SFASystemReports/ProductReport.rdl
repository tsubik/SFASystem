        <Field Name="Tax">
          <DataField>Tax</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Price">
          <DataField>Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT prod.Code, prod.Name as ProdName, prodGr.Name as ProdGrName, man.Name as ManName, dict.Value as Tax, prod.Price
FROM [Product] prod LEFT JOIN [Manufacturer] man ON prod.ManufacturerID=man.ManufacturerID
	JOIN [Dictionary] dict ON prod.Tax=dict.DictionaryID
	JOIN [ProductGroup] prodGr ON prod.ProductGroupID=prodGr.ProductGroupID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
