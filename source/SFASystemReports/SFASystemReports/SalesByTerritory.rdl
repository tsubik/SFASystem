      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT terr.Name, SUM(ord.Total) as SumTotal FROM [Order] ord
JOIN [Territory] terr ON ord.TerritoryID=terr.TerritoryID
WHERE ord.OrderDate between @startdate and @enddate
GROUP BY terr.Name
ORDER BY SumTotal DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@startdate">
            <Value>=Parameters!startdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@enddate">
            <Value>=Parameters!enddate.Value</Value>
