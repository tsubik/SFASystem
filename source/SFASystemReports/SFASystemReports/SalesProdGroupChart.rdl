  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SumTotal">
          <DataField>SumTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT prodGr.Name, SUM(ord.Total) as SumTotal FROM [Order] ord
JOIN [OrderLine] ordLine ON ord.OrderID = ordLine.OrderID
JOIN [Product] prod ON ordLine.ProductID=prod.ProductID
JOIN [ProductGroup] prodGr ON prod.ProductGroupID=prodGr.ProductGroupID
JOIN [Invoice] inv ON ord.OrderID=inv.OrderID
WHERE inv.IsPaid = 1
GROUP BY prodGr.Name
ORDER BY SumTotal DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
